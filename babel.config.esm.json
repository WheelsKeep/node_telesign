{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "esmodules": true
                },
                "modules": false
            }
        ]
    ],
    "plugins": [
        [ "@babel/plugin-syntax-import-assertions" ],
        [ "./babel-plugin.js" ],
        [
            "babel-plugin-replace-import-extension",
            {
                "extMapping": {
                    ".mjs": ""
                }
            }
        ]
    ]
}
